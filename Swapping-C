#include <stdio.h>
int main() 
{
int a = 5, b = 10;
printf("Before swapping (without third variable): a = %d, b = %d\n", a, b);
a = a + b;
b = a - b;
a = a - b;
printf(" After swapping (without third variable): a = %d, b = %d\n", a, b);
int x = 15, y = 20, temp;
printf("Before swapping (with third variable): x = %d, y = %d\n", x, y);
temp=x;
x=y;
y=temp;
printf("After swapping(with third variable):x=%d,y=%d\n",x,y);
return 0;
}
